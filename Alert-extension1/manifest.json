{
    "name": "Alert",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"},
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "<all_urls>",
        "activeTab",
        "contextMenus",
        "tabs",
        "notifications",
        "storage"
      ]
}